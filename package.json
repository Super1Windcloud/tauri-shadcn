  "version": "0.1.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "td": "tauri dev ",
    "tb": "tauri build",
    "taze": "npx taze major -w -I",
    "shadcn": "npx shadcn@latest add -a -y -o &&   npx shadcn@latest diff",
    "cargo-update": "cd src-tauri && cargo upgrade --incompatible",
    "clean": "npx rimraf node_modules",
    "cargo-clean": "cd src-tauri && cargo clean",
    "lint": "eslint  src --fix && cd src-tauri && cargo check",
    "format": "prettier --write src && cd src-tauri &&  cargo fmt",
    "icon": "tauri icon  app-icon.png",
    "bump": "pnpm taze && pnpm cargo-update && pnpm shadcn"
  },
