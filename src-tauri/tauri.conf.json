{
	"productName": "tauri-ui",
	"version": "0.1.2",
	"identifier": "com.superwindcloud.tauri-ui",
	"build": {
		"beforeDevCommand": "pnpm dev",
		"beforeBuildCommand": "pnpm build",
		"devUrl": "http://localhost:1420",
		"frontendDist": "../dist"
	},
	"app": {
		"security": {
			"csp": null,
			"capabilities": [
				"default"
			]
		},
		"windows": [
			{
				"label": "main",
				"fullscreen": false,
				"title": "tauri-ui",
				"center": true,
				"resizable": true,
				"decorations": false,
				"transparent": true,
				"height": 600,
				"width": 800,
				"visible": false
			}
		],
		"macOSPrivateApi": true
	},
	"bundle": {
		"active": true,
		"targets": "all",
		"icon": [
			"icons/32x32.png",
			"icons/128x128.png",
			"icons/128x128@2x.png",
			"icons/icon.icns",
			"icons/icon.ico"
		],
		"macOS": {
			"entitlements": "entitlements.plist"
		}
	}
}